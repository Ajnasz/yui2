<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="9" skipfailedinvocationCounts="false" verbose="1" name="Failed suite [SelNG Test]" junit="false" parallel="methods" annotations="JDK">
  <parameter name="config-file" value="src/test/resources/config_grid.properties"/>
  <parameter name="config-file_YUI" value="src/test/resources/config_grid.properties"/>
  <listeners>
    <listener class-name="com.yahoo.test.SelNG.framework.util.MethodSorter"/>
    <listener class-name="com.yahoo.test.SelNG.framework.util.SelNGEmailReporter"/>
    <listener class-name="org.uncommons.reportng.HTMLReporter"/>
    <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    <listener class-name="com.yahoo.test.SelNG.framework.util.TMReporter"/>
    <listener class-name="com.yahoo.test.SelNG.framework.util.Customreport"/>
  </listeners>
  <test name="YUI(failed)" junit="false" parallel="methods" annotations="JDK">
    <parameter name="config-file" value="src/test/resources/config_grid.properties"/>
    <parameter name="config-file_YUI" value="src/test/resources/config_grid.properties"/>
    <groups>
      <run>
        <include name="abstract"/>
        <include name="grid"/>
        <include name="demo"/>
        <exclude name="non-grid"/>
      </run>
    </groups>
    <classes>
      <class name="com.yahoo.test.SelNG.YUI.tests.ContainerTestOverlay">
        <methods>
          <include name="setupTestGrid"/>
          <include name="TabView"/>
          <include name="initTestInGrid"/>
          <include name="tearDownTestGrid"/>
        </methods>
      </class>
    </classes>
  </test>
</suite>
